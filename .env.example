# App
APP_NAME=monitoring-dashboard
SERVER_PORT=8080

# Database
DATABASE_URL=jdbc:postgresql://localhost:5432/monitoring_db
DATABASE_USERNAME=postgres
DATABASE_PASSWORD=your_password_here

# JWT - MUST be at least 64 characters for HS512
JWT_SECRET=your_64_character_secret_key_here
JWT_EXPIRATION=86400000

# JPA
JPA_DDL_AUTO=update
JPA_SHOW_SQL=true

# Logs
LOG_SECURITY_LEVEL=DEBUG
LOG_APP_LEVEL=DEBUG